<Window x:Class="suzhou.Logout"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:suzhou"
        mc:Ignorable="d"
        Title="Logout" Height="200" Width="300"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="125*"/>
            <ColumnDefinition Width="53*"/>
            <ColumnDefinition Width="114*"/>
        </Grid.ColumnDefinitions>
        <TextBlock HorizontalAlignment="Left" Margin="62,35,0,0" TextWrapping="Wrap" Text="确定注销吗？" FontSize="30"  VerticalAlignment="Top" Grid.ColumnSpan="3"/>
        <Button Content="确定" HorizontalAlignment="Left" Margin="63,100,0,0" VerticalAlignment="Top" FontSize="20" Width="75" Click="btn_Ok_Click" Grid.ColumnSpan="2"/>
        <Button Content="取消" HorizontalAlignment="Left" Margin="33.081,100,0,0" VerticalAlignment="Top" FontSize="20" Width="75" RenderTransformOrigin="1.83,0.434" Click="btn_Cancel_Click" Grid.ColumnSpan="2" Grid.Column="1"/>

    </Grid>
</Window>
